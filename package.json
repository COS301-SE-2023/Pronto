{
  "dependencies": {
    "@aws-amplify/cli": "^12.6.0"
  },
  "scripts": {
    "dev:backend": "amplify env checkout dev ",
    "beta:backend": "amplify env checkout beta",
    "prod:backend": "amplify env checkout prod && git switch prod",
    "deploy:backend": "amplify push",
    "pull:backend": "amplify pull",
    "status:backend": "amplify status",
    "update:backend:function": "amplify update function",
    "prod:config:backend:function": "aws lambda update-function-configuration --cli-input-yaml ",
    "status:backend:getfunction": "aws lambda get-function --function-name ",
    "status:backend:auth:userpool": "aws cognito-idp describe-user-pool --cli-input-yaml",
    "status:backend:auth:userpool-client": "aws cognito-idp describe-user-pool-client --cli-input-yaml"
  }
}
