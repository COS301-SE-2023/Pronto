[
  {
    "Action": ["cognito-idp:AdminUpdateUserAttributes"],
    "Resource": [
      {
        "Fn::Join": [
          "",
          [
            "arn:aws:cognito-idp:",
            {
              "Ref": "AWS::Region"
            },
            ":",
            {
              "Ref": "AWS::AccountId"
            },
            ":userpool/",
            {
              "Ref": "authprontoAuthUserPoolId"
            }
          ]
        ]
      }
    ]
  },
  {
    "Action": ["dynamodb:GetItem"],
    "Resource": [
      {
        "Fn::Join": [
          "",
          [
            "arn:aws:dynamodb:",
            {
              "Ref": "AWS::Region"
            },
            ":",
            {
              "Ref": "AWS::AccountId"
            },
            ":table/institution-",
            {
              "Ref": "authprontoAuthUserPoolId"
            },
            "-${env}"
          ]
        ]
      }
    ]
  }
]
